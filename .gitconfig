[color]
      ui = true

[color "branch"]
      current = yellow reverse
      local = yello
      remote = green

[color "diff"]
      meta = yellow bold
      frag = magenta bold
      old = red bold
      new = green bold

[color "status"]
      added = yellow
      changed = green
      untracked = red

[core]
      autocrlf = input
      compression = 9
      excludesfile = ~/.gitignore_global
      filemode = false

[diff]
      mnemonicprefix = true

[merge]
      log = true
      ff = false

[push]
      default = simple

[url "https://github.com/"]
      insteadOf = gh

[url "https://gist.github.com/"]
      insteadOf = gist::

[url "https://bitbucket.org/"]
      insteadOf = bb:

[user]
      name = Kudzai Tuso
